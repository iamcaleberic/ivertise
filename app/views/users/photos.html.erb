<%= render 'layouts/menu' %>
<div class="container" style="margin-top: 5rem;">
	<div class="row">
		<div id="masonry-grid">
			<% @photos.each do |photo| %>
			<div id="picha">
				<div class="ui card">
					<div class="image">
		  				<%#= photo.inspect %>
						<%= image_tag photo.image(:medium) %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>
<script type="text/javascript">
	var $container = jQuery('#masonry-grid');
    // initialize
    $container.masonry({
      columnWidth: 50,
      itemSelector: '#picha'
    });
</script>